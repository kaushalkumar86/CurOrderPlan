<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<manifest 
    xmlns:android="http://schemas.android.com/apk/res/android" 
    android:versionCode="2" 
    android:versionName="8.19.102" 
    package="com.kaushal.cutorderplan">

    <uses-sdk android:minSdkVersion="8" android:targetSdkVersion="19"/>
     
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
  	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
  	

    <application android:allowBackup="false" android:icon="@drawable/cutplan" android:label="@string/app_name" android:name="com.kaushal.cutorderplan.MainApp" android:theme="@android:style/Theme.Holo">
        
        <meta-data android:name="com.amazon.mobile.sdk.plugin" android:value="identity"/>
        
        <activity android:label="@string/title_activity_main" android:name="com.kaushal.cutorderplan.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.kaushal.cutorderplan.EditStyleActivity"/>
        <activity android:name="com.kaushal.cutorderplan.MarkerResultActivity"/>
        <activity android:name="com.kaushal.cutorderplan.MarkerResultDetailActivity"/>
        <activity android:name="com.kaushal.cutorderplan.OpenPDFActivity"/>
        <activity android:configChanges="keyboardHidden|orientation|screenSize" android:name="com.amazon.device.ads.AdActivity"/>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
</manifest>
